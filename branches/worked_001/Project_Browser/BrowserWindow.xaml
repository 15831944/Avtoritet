        xmlns:extensions="clr-namespace:BrowserExtension.Extensions"
        Title="BrowserWindow" Height="300" Width="300">
    <Grid>
        <WindowsFormsHost Name="IeHost" Panel.ZIndex="10" Background="{x:Null}">
            <extensions:ExtendedWebBrowser x:Name="IeWeb" />
        </WindowsFormsHost>
    </Grid>
</Window>
